{
  "name": "swagger-mocker",
  "version": "0.0.1",
  "description": "根据swagger文档生成mock数据，并启动http数据mock服务",
  "main": "lib/cmd.js",
  "bin": {
    "mocker": "bin/index.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "test": "npm run build && node ./bin/index.js start",
    "build": "rm -rf lib && tsc",
    "publish": "npm run build && nvm use npm && npm publish",
    "prepare": "husky install"
  },
  "files": [
    "src",
    "lib",
    "*.md",
    "bin",
    "template"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "translation.js": "^0.7.15",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.167",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.21",
    "husky": "^5.2.0",
    "ts-node": "^9.1.1"
  },
  "author": "stoly",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiweixiong/swagger-mocker"
  },
  "bugs": {
